{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-strongly-recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "vue/attribute-hyphenation": "error",
    "vue/component-definition-name-casing": [ "warn", "kebab-case" ],
    "vue/html-quotes": [ "error", "double", { "avoidEscape": false } ]
  }
}